{
  "dataset": {
    "name": "gqa",
    "features": ["questions"],
    "filemap": {
      "root": "./data/gqa"
    },
    "train": {
      "split": "train",
      "version": "balanced"
    },
    "val": {
      "split": "val",
      "version": "balanced"
    },
    "test": {
      "split": "testdev",
      "version": "balanced"
    }
  },
  "preprocessing": {
    "cache": "./cache",
    "pipeline": [{
      "feature": "questions"
    }]
  },
  "model": {
    "optimiser": {
      "learning_rate": 0.001,
      "weight_decay": 1e-5
    }
  },
  "training": {
    "epochs": 25,
    "log_step": 16
  },
  "dataloader": {
    "batch_size": 256,
    "workers": 0
  }
}
