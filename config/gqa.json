{
  "dataset": {
    "name": "gqa",
    "features": ["questions"],
    "filemap": {
      "root": "./data/gqa"
    }
  },
  "preprocessing": {
    "cache": {
      "root": "./cache",
      "artifact": "gqa-preprocessed-dataset"
    },
    "pipeline": [{
      "feature": "questions",
      "split": "train",
      "version": "balanced"
    },
    {
      "feature": "questions",
      "split": "val",
      "version": "balanced"
    },
    {
      "feature": "questions",
      "split": "testdev",
      "version": "balanced"
    }]
  },
  "model": {
    "data": {
      "artifact": "gqa-preprocessed-dataset:latest",
      "train": {
        "split": "train",
        "version": "balanced"
      },
      "val": {
        "split": "val",
        "version": "balanced"
      },
      "test": {
        "split": "testdev",
        "version": "balanced"
      }
    },
    "optimiser": {
      "learning_rate": 0.001,
      "weight_decay": 1e-5
    }
  },
  "training": {
    "epochs": 25,
    "log_step": 16
  },
  "dataloader": {
    "batch_size": 256,
    "workers": 0
  }
}
