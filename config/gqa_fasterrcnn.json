{
  "dataset": {
    "name": "gqa",
    "filemap": {
      "root": "./data/gqa"
    }
  },
  "preprocessing": {
    "cache": {
      "root": "./cache",
      "artifact": "gqa-rcnn"
    },
    "pipeline": [{
      "feature": "scene_graphs",
      "split": "train"
    },
    {
      "feature": "scene_graphs",
      "split": "val"
    }]
  },
  "model": {
    "name": "faster_rcnn",
    "pretrained": false,
    "backbone": {
      "name": "resnet50",
      "pretrained": true
    }
  },
  "training": {
    "epochs": 25,
    "log_step": 128,
    "eval_subset": 32,
    "dataloader": {
      "batch_size": 2,
      "workers": 1
    },
    "optimiser": {
      "name": "sgd",
      "learning_rate": 0.001,
      "momentum": 0.9,
      "weight_decay": 5e-4
    },
    "data": {
      "features": [
        {
          "name": "scene_graphs",
          "artifact": "gqa-rcnn:latest"
        },
        {
          "name": "images",
          "artifact": null
        }
      ],
      "train": {
        "split": "train",
        "version": "balanced"
      },
      "val": {
        "split": "val",
        "version": "balanced"
      },
      "test": {
        "split": "testdev",
        "version": "balanced"
      }
    }
  }
}
